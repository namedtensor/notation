\RequirePackage{amssymb}

\newcommand{\name}[1]{\ensuremath{\mathsf{#1}}}
\newcommand{\nbin}[2]{\mathbin{\underset{\substack{#1}}{#2}}}
\newcommand{\ndot}[1]{\nbin{#1}{\odot}}
\newcommand{\ncat}[1]{\nbin{#1}{\oplus}}
\newcommand{\nsum}[1]{\sum\limits_{\substack{#1}}}
\newcommand{\nfun}[2]{\mathop{\underset{\substack{#1}}{\mathrm{#2}}}}
\newcommand{\ndef}[2]{\newcommand{#1}{\name{#2}}}
